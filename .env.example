# AWS Configuration
AWS_REGION=""
AWS_ACCESS_KEY_ID=""
AWS_SECRET_ACCESS_KEY=""

# RDS Data API Configuration
RDS_CLUSTER_ARN=""
RDS_SECRET_ARN=""
RDS_DATABASE_NAME=""

# Proxy Server Configuration
PROXY_PORT=5432
PROXY_HOST=localhost

# Logging Configuration
LOG_LEVEL=debug 
LOG_FILE=logs/proxy.log

# PostgreSQL Mock Settings
MOCK_PG_VERSION=14.9
MOCK_SERVER_VERSION=PostgreSQL 14.9 on x86_64-pc-linux-gnu

# Multi-threading Configuration
USE_CLUSTERING=true
NUM_WORKERS=4
MAX_THREADS=4

# Performance Tuning
WORKER_IDLE_TIMEOUT=30000
TASK_TIMEOUT=60000
MAX_RESTARTS=5
RESTART_DELAY=1000